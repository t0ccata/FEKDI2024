{
    "version": 2,
    "builds": [
      {
        "src": "api/app.py",
        "use": "@vercel/python",
        "config": {
          "zeroConfig": true,
          "buildCommand": "pip install --upgrade pip && pip install -r api/requirements.txt"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/api/app.py"
      }
    ]
  }
  